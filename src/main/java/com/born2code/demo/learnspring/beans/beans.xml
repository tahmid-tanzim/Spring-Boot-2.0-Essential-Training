<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	default-init-method="init" default-destroy-method="destroy"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="patient" class="com.born2code.demo.learnspring.Patient"
		scope="singleton" init-method="onCreate" destroy-method="onDestroy"
		c:id="9" c:name="Yellow">
		<property name="emergencyContact">
			<list>
				<value>Name1</value>
				<value>Name2</value>
				<value>Name3</value>
				<value>Name4</value>
				<value>Name5</value>
			</list>
		</property>
	</bean>

	<bean class="com.born2code.demo.learnspring.PatientFactory" id="patientfactory">
	</bean>
</beans>
